"{\"Section\":{\"0\":\"CPE\\u00a0100\",\"1\":\"CPE\\u00a0101\",\"2\":\"CPE\\u00a0102\",\"3\":\"CPE\\u00a0103\",\"4\":\"CPE\\u00a0105\",\"5\":\"CPE\\u00a0108\",\"6\":\"CPE\\u00a0123\",\"7\":\"CPE\\u00a0133\",\"8\":\"CPE\\u00a0200\",\"9\":\"CPE\\u00a0209\",\"10\":\"CPE\\u00a0225\",\"11\":\"CPE\\u00a0233\",\"12\":\"CPE\\u00a0235\",\"13\":\"CPE\\u00a0236\",\"14\":\"CPE\\u00a0290\",\"15\":\"CPE\\u00a0300\",\"16\":\"CPE\\u00a0301\",\"17\":\"CPE\\u00a0305\",\"18\":\"CPE\\u00a0307\",\"19\":\"CPE\\u00a0308\",\"20\":\"CPE\\u00a0309\",\"21\":\"CPE\\u00a0315\",\"22\":\"CPE\\u00a0321\",\"23\":\"CPE\\u00a0328\",\"24\":\"CPE\\u00a0329\",\"25\":\"CPE\\u00a0336\",\"26\":\"CPE\\u00a0344\",\"27\":\"CPE\\u00a0349\",\"28\":\"CPE\\u00a0350\",\"29\":\"CPE\\u00a0357\",\"30\":\"CPE\\u00a0365\",\"31\":\"CPE\\u00a0366\",\"32\":\"CPE\\u00a0368\",\"33\":\"CPE\\u00a0369\",\"34\":\"CPE\\u00a0378\",\"35\":\"CPE\\u00a0400\",\"36\":\"CPE\\u00a0402\",\"37\":\"CPE\\u00a0405\",\"38\":\"CPE\\u00a0406\",\"39\":\"CPE\\u00a0409\",\"40\":\"CPE\\u00a0416\",\"41\":\"CPE\\u00a0419\",\"42\":\"CPE\\u00a0428\",\"43\":\"CPE\\u00a0430\",\"44\":\"CPE\\u00a0431\",\"45\":\"CPE\\u00a0432\",\"46\":\"CPE\\u00a0435\",\"47\":\"CPE\\u00a0436\",\"48\":\"CPE\\u00a0437\",\"49\":\"CPE\\u00a0439\",\"50\":\"CPE\\u00a0441\",\"51\":\"CPE\\u00a0448\",\"52\":\"CPE\\u00a0450\",\"53\":\"CPE\\u00a0453\",\"54\":\"CPE\\u00a0454\",\"55\":\"CPE\\u00a0458\",\"56\":\"CPE\\u00a0461\",\"57\":\"CPE\\u00a0462\",\"58\":\"CPE\\u00a0464\",\"59\":\"CPE\\u00a0465\",\"60\":\"CPE\\u00a0466\",\"61\":\"CPE\\u00a0468\",\"62\":\"CPE\\u00a0470\",\"63\":\"CPE\\u00a0471\",\"64\":\"CPE\\u00a0472\",\"65\":\"CPE\\u00a0473\",\"66\":\"CPE\\u00a0474\",\"67\":\"CPE\\u00a0476\",\"68\":\"CPE\\u00a0478\",\"69\":\"CPE\\u00a0479\",\"70\":\"CPE\\u00a0480\",\"71\":\"CPE\\u00a0481\",\"72\":\"CPE\\u00a0482\",\"73\":\"CPE\\u00a0483\",\"74\":\"CPE\\u00a0484\",\"75\":\"CPE\\u00a0485\",\"76\":\"CPE\\u00a0486\",\"77\":\"CPE\\u00a0488\",\"78\":\"CPE\\u00a0489\",\"79\":\"CPE\\u00a04